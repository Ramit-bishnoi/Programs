    }
    printf("___________________\n");

    for(i=1;i<=5;i++){
        for(j=1;j<=5;j++)
        printf("%d ",j);
        printf("\n");
    }
    printf("___________________\n");

    int k=0;
     for(i=1;i<=5;i++){
        for(j=1;j<=5;j++)
