#include<stdio.h>

int main()
{
    int i,j, k=0;
    for(i=1;i<=5;i++){
      for(j=5;j>i-1;j--)
        printf("*");
        printf("\n");
    }
    printf("_________________\n");
    for(i=1;i<=5;i++){
        for(j=5;j>i-1;j--)
        printf("%d",i);
        printf("\n");
    }
    printf("_________________\n");

    for(i=1;i<=5;i++){
        for(j=5;j>i-1;j--)
        printf("%d",j);
        printf("\n");
    }
    printf("_________________\n");

    for(i=1;i<=5;i++){
        for(j=5;j>i-1;j--)
        printf("%d\t",k++);
        printf("\n");
    }
    printf("_________________\n");

    for(i=1;i<=5;i++){
        for(j=5;j>i-1;j--)
        printf("%c",i + 64);
        printf("\n");
    }
    printf("_________________\n");

    for(i=1;i<=5;i++){
        for(j=5;j>i-1;j--)
        printf("%c",j + 64);
        printf("\n");
    }
}
#include <stdio.h>

void selectionSort(int arr[], int n) {
    int i, j, minIndex, temp;

    for (i = 0; i < n - 1; i++) {
        minIndex = i;
        for (j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIndex]) {
                minIndex = j;
            }
        }
        temp = arr[minIndex];
        arr[minIndex] = arr[i];
        arr[i] = temp;
    }
}

int main() {
    int arr[] = {64, 34, 25, 12, 22, 11, 90};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Original Array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    selectionSort(arr, n);

    printf("\nSorted Array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
